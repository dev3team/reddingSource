  },
  "homepage": "https://creativedotdesign.com",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://opensource.org/licenses/MIT"
    }
  ],
  "browserslist": "last 2 Chrome versions, last 2 Firefox versions, last 2 safari versions",
